// Place your settings in this file to overwrite the default settings
{
    "[dart]": {
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.rulers": [
            80
        ],
        "editor.selectionHighlight": false,
        "editor.suggest.snippetsPreventQuickSuggestions": false,
        "editor.suggestSelection": "first",
        "editor.tabCompletion": "onlySnippets",
        "editor.wordBasedSuggestions": "off"
    },
    "[markdown]": {
        "editor.quickSuggestions": {
            "comments": "off",
            "other": "off",
            "strings": "off"
        },
        "editor.wordWrap": "on",
        "files.trimTrailingWhitespace": false
    },
    "[rust]": {
        "editor.formatOnSave": true
    },
    "code-eol.crlfCharacter": "¤¬",
    "code-eol.newlineCharacter": "¬",
    "code-eol.returnCharacter": "¤",
    "diffEditor.ignoreTrimWhitespace": false,
    "docker.dockerodeOptions": {
        "socketPath": "/run/user/1000/podman/podman.sock"
    },
    "editor.acceptSuggestionOnEnter": "smart",
    "editor.copyWithSyntaxHighlighting": false,
    "editor.fontFamily": "'IntoneMono Nerd Font Mono', 'SauceCodePro Nerd Font Mono', 'SauceCodePro Nerd Font', 'Droid Sans Mono', 'monospace', monospace",
    "editor.fontSize": 16,
    "editor.guides.bracketPairs": "active",
    "editor.minimap.enabled": false,
    "editor.quickSuggestions": {
        "other": "off"
    },
    "editor.renderWhitespace": "all",
    "editor.rulers": [
        80
    ],
    "editor.tabSize": 2,
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "constant.character.escape",
                "settings": {
                    "fontStyle": "",
                    "foreground": "#008000"
                }
            }
        ]
    },
    "editor.trimAutoWhitespace": true,
    "eslint.ignoreUntitled": true,
    "extensions.ignoreRecommendations": false,
    "files.associations": {
        "*.ddl": "oracle-sql",
        "*.pkb": "oracle-sql",
        "*.pks": "oracle-sql",
        "*.sql": "oracle-sql"
    },
    "files.defaultLanguage": "oracle-sql",
    "files.trimTrailingWhitespace": true,
    "git.confirmSync": false,
    "git.ignoreLegacyWarning": true,
    "git.replaceTagsWhenPull": true,
    "git.suggestSmartCommit": false,
    "gitlens.advanced.messages": {
        "suppressLineUncommittedWarning": true
    },
    "gremlins.showInProblemPane": true,
    "java.saveActions.organizeImports": true,
    "material-icon-theme.files.associations": {
        ".work_aliases": "Console",
        ".work_env": "Console"
    },
    "material-icon-theme.folders.associations": {
        ".oci": "Config"
    },
    "redhat.telemetry.enabled": true,
    "security.workspace.trust.untrustedFiles": "open",
    "svn.enableProposedApi": "none",
    "telemetry.telemetryLevel": "off",
    "terminal.integrated.enableShellIntegration": true,
    "terminal.integrated.env.linux": {
        "SHLVL": "0"
    },
    "terminal.integrated.fontSize": 20,
    "terminal.integrated.shellIntegration.enabled": false,
    "update.mode": "none",
    "vsicons.dontShowNewVersionMessage": true,
    "window.restoreWindows": "none",
    "workbench.colorCustomizations": {
        "terminal.background": "#000",
        "terminal.foreground": "#fff",
        "terminal.selectionBackground": "#a38e8e"
    },
    "workbench.colorTheme": "GitHub Light",
    "workbench.editor.customLabels.patterns": {
        "**/nonprod/**/unit_test/package_body/*.sql": "ut body - ${filename}.${extname} (unit_test)",
        "**/nonprod/**/unit_test/package_spec/*.sql": "ut spec - ${filename}.${extname} (unit_test)",
        "**/product/**/package_body/*.sql": "body - ${filename}.${extname} (${dirname(1)})",
        "**/product/**/package_spec/*.sql": "spec - ${filename}.${extname} (${dirname(1)})"
    },
    "workbench.editor.limit.enabled": true,
    "workbench.editor.limit.value": 5,
    "workbench.iconTheme": "material-icon-theme",
    "yaml.customTags": [
        "!reference sequence"
    ],
    "yaml.schemas": {
        "https://json.schemastore.org/yamllint.json": "deploy.yaml"
    },
    "editor.unicodeHighlight.nonBasicASCII": false,
    "alignment.chars": {

    ":": {
      "spaceBefore": 0,
      "spaceAfter": 1
    },
    ":=": {
      "spaceBefore": 1,
      "spaceAfter": 1
    },
    "===": {
      "spaceBefore": 1,
      "spaceAfter": 1
    },
    "==": {
      "spaceBefore": 1,
      "spaceAfter": 1
    },
    "=>": {
      "spaceBefore": 1,
      "spaceAfter": 1
    },
    " in ": {
      "spaceBefore": 0,
      "spaceAfter": 0
    }
},
"terminal.integrated.enableMultiLinePasteWarning": false,
"sqldeveloper.database.nls.territory": "AUSTRALIA",
"sqldeveloper.database.nls.dateFormat": "DD-MON-RRRR",
"sqldeveloper.database.nls.timestampFormat": "DD-MON-RRRR HH24:MI:SSXFF",
"sqldeveloper.database.nls.timestampTZ Format": "DD-MON-RRRR HH24:MI:SSXFF TZR",
"sqldeveloper.database.nls.ISOCurrency": "AUSTRALIA",
"sqldeveloper.sqlHistory.historyLimit": 500,
"extensions.autoUpdate": true,
"git.blame.editorDecoration.enabled": false,
"containers.containerClient": "com.microsoft.visualstudio.containers.podman",
}